{
  "name": "elm-widgets-alpha",
  "version": "7.0.2",
  "description": "A collection of widgets themed through ThemeSpec.",
  "scripts": {
    "test": "elm-test-rs tests/**/*.elm",
    "start": "concurrently \"npm:watch:*\"",
    "watch:styles": "watch 'node ./styles/build-styles.js' ./styles/output",
    "watch:tailwind": "tailwindcss -i ./styles/index.css -o ./styles/output/index.css --watch",
    "min:watch:tailwind": "tailwindcss -i ./styles/index.css -o ./styles/output/index.css --minify --watch",
    "start:old": "watch 'node ./styles/build-styles.js' ./styles",
    "docs": "elm-doc-preview"
  },
  "author": "Georges Boris",
  "license": "MPL-2.0",
  "devDependencies": {
    "elm-theme-tailwindcss": "^1.0.1",
    "autoprefixer": "^10.4.1",
    "concurrently": "^7.1.0",
    "cssnano": "^5.0.14",
    "elm-doc-preview": "^5.0.5",
    "elm-tooling": "^1.7.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^3.1.8",
    "watch": "^1.0.2"
  }
}
